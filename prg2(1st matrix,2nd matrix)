#include<stdio.h>
void main()
{
    int a[3][3];
    int b[3][3];
    int c[3][3];
    printf("enter elements of first matrix:\n");
    for(int i=0;i<=2;i++)
    {
        for(int j=0;j<=2;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    printf("enter elements of second matrix:\n");
    for(int i=0;i<=2;i++)
    {
        for(int j=0;j<=2;j++)
        {
            scanf("%d",&b[i][j]);
        }
    }
    printf("===first matrix===");
    for( int i=0;i<=2;i++)
    {
        for(int j=0;j<=2;j++)
        {
            printf("%d",a[i][j]);
        }
        printf("\n");
    }
    printf("===second matrix===");
    {
        for(int i=0;i<=2;i++)
        {
            for(int j=0;j<=2;j++)
            {
                printf("%d",b[i][j]);
            }
            printf("\n");
        }
    }
    
}
